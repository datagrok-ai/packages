{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "max-len": [
      "error",
      120
    ],
    "no-unused-vars": "warn",
    "require-jsdoc": "off",
    "valid-jsdoc": "warn",
    "spaced-comment": "off",
    "linebreak-style": "off"
  },
  "plugins": [
    "@typescript-eslint"
  ]
}