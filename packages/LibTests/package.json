{
  "name": "@datagrok/lib-tests",
  "friendlyName": "Lib Tests",
  "version": "1.0.0",
  "description": "Automated tests for libraries",
  "dependencies": {
    "datagrok-api": "latest",
    "cash-dom": "latest",
    "dayjs": "latest",
    "@datagrok-libraries/utils": "../../libraries/utils",
    "rxjs": "^6.6.7",
    "wu": "latest"
  },
  "devDependencies": {
    "@types/wu": "^2.1.42",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
