{
  "name": "@datagrok/chem",
  "friendlyName": "Chem",
  "fullName": "Chem",
  "version": "1.9.0",
  "author": {
    "name": "Leonid Stolbov",
    "email": "lstolbov@datagrok.ai"
  },
  "description": "Cheminformatics support: import, rendering, sketching, calculation of properties, predictive models, augmentations, multiple analyses.",
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/Chem"
  },
  "properties": [
    {
      "name": "Renderer",
      "propertyType": "string",
      "choices": [
        "RDKit",
        "OpenChemLib"
      ],
      "defaultValue": "RDKit",
      "nullable": false
    },
    {
      "name": "Sketcher",
      "propertyType": "string",
      "choices": [
        "OpenChemLib",
        "Ketcher",
        "Marvin",
        "ChemDraw"
      ],
      "defaultValue": "",
      "nullable": true
    }
  ],
  "dependencies": {
    "datagrok-api": "^1.18.0",
    "@datagrok-libraries/chem-meta": "^1.2.2",
    "@datagrok-libraries/ml": "^6.4.9",
    "@datagrok-libraries/utils": "^4.1.44",
    "@datagrok-libraries/tutorials": "^1.3.6",
    "typescript-lru-cache": "latest",
    "cash-dom": "latest",
    "dayjs": "latest",
    "file-loader": "latest",
    "jstat": "^1.9.5",
    "offscreen-canvas": "latest",
    "openchemlib": "^7.2.3",
    "rxjs": "^6.5.5",
    "typescript": "latest",
    "worker-loader": "latest",
    "source-map-loader": "^4.0.1",
    "wu": "latest"
  },
  "devDependencies": {
    "@types/wu": "latest",
    "@types/offscreencanvas": "latest",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.1.0",
    "eslint-config-google": "^0.14.0",
    "path": "^0.12.7",
    "webpack": "latest",
    "webpack-cli": "latest",
    "style-loader": "latest",
    "css-loader": "latest",
    "ts-loader": "latest",
    "@datagrok/chembl": "^1.0.35",
    "@datagrok/pubchem-api": "^1.1.16",
    "@datagrok/chembl-api": "^1.0.12"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "crypto": false
  },
  "scripts": {
    "link-api": "npm link datagrok-api",
    "link-utils": "npm link @datagrok-libraries/utils",
    "link-ml": "npm link @datagrok-libraries/ml",
    "link-chem-meta": "npm link @datagrok-libraries/chem-meta",
    "link-all": "npm link @datagrok-libraries/chem-meta datagrok-api @datagrok-libraries/utils @datagrok-libraries/ml @datagrok-libraries/tutorials",
    "build-chem-local": "npm --prefix ./../../js-api run build && npm --prefix ./../../libraries/utils run build && npm --prefix ./../../libraries/ml run build && npm run build",
    "build": "webpack",
    "debug-local": "tsc && webpack && grok publish local",
    "release-local": "grok publish local --release",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint-fix": "eslint \"./src/**/*.ts\" --fix",
    "test": "grok test",
    "test-dev": "grok test --host dev",
    "test-local": "grok test --host localhost",
    "build-all": "npm --prefix ./../../libraries/chem-meta run build && npm --prefix ./../../js-api run build && npm --prefix ./../../libraries/utils run build && npm --prefix ./../../libraries/ml run build && npm --prefix ./../../libraries/tutorials run build && npm run build"
  },
  "canEdit": [
    "Developers"
  ],
  "canView": [
    "All users"
  ],
  "category": "Cheminformatics",
  "meta": {
    "dartium": false,
    "browserFeatures": [
      "wasm"
    ]
  }
}
