{
  "name": "@datagrok/chembl",
  "version": "1.1.0",
  "description": "Chembl integration, commonly used queries and browser",
  "author": {
    "name": "Maria Dolotova",
    "email": "mdolotova@datagrok.ai"
  },
  "fullName": "Chembl",
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/Chembl"
  },
  "dependencies": {
    "datagrok-api": "^1.19.0",
    "@datagrok-libraries/utils": "^4.3.0",
    "@types/react-dom": "^17.0.9",
    "file-loader": "^6.2.0",
    "ts-loader": "latest",
    "typescript": "latest"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "css-loader": "latest",
    "eslint": "^8.21.0",
    "eslint-config-google": "^0.14.0",
    "style-loader": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "@datagrok/chem": "^1.12.0"
  },
  "scripts": {
    "link-api": "npm link datagrok-api",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "build": "webpack",
    "test": "grok test",
    "test-dev": "grok test --host dev",
    "test-local": "grok test --host localhost"
  },
  "canEdit": [
    "Developers"
  ],
  "canView": [
    "All users"
  ],
  "category": "Cheminformatics",
  "meta": {
    "semanticTypes": [
      {
        "semType": "CHEMBL_ID",
        "description": "Compound id in the CHEMBL database",
        "parsers": [
          {
            "regexp": "CHEMBL\\d+"
          }
        ]
      }
    ]
  }
}
