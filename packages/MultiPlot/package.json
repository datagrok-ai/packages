{
	"name": "@datagrok/multiplot",
	"friendlyName": "MultiPlot",
	"version": "0.0.4",
	"beta": true,
	"description": "MultiPlot is a [package](https://datagrok.ai/help/develop/develop#packages) for the [Datagrok](https://datagrok.ai) platform. MultiPlot was developed in purpose to provide to user ability to easily create dashboards based on complex data which have common influencing parameter (usually presented as axis X on charts). Common axis X combined with compact form of presenting charts allow user quickly estimate data meaning.",
	"repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/MultiPlot"
  },
	"dependencies": {
		"chokidar": "^3.5.2",
		"datagrok-api": "^1.0.0",
		"typescript": "^4.3.5",
		"ws": "^7.5.1"
	},
	"scripts": {
		"debug-multiplot": "grok publish --rebuild",
		"release-multiplot": "grok publish --rebuild --release",
		"build-multiplot": "webpack",
		"build": "webpack",
		"debug-multiplot-dev": "grok publish dev --rebuild",
		"release-multiplot-dev": "grok publish dev --rebuild --release",
		"debug-multiplot-public": "grok publish public --rebuild",
		"release-multiplot-public": "grok publish public --rebuild --release"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.28.2",
		"@typescript-eslint/parser": "^4.28.2",
		"echarts": "^5.1.2",
		"eslint": "^7.30.0",
		"eslint-config-google": "^0.14.0",
		"ts-loader": "^9.2.3",
		"webpack": "^5.45.1",
		"webpack-cli": "^4.7.2"
	},
	"canEdit": [
		"Developers"
	],
	"canView": [
		"All users"
	],
  "category": "Visualizations"
