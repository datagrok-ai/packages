{
  "name": "@datagrok/charpy",
  "fullName": "ChaRPy",
  "version": "1.0.5",
  "author": {
    "name": "Alexander Paramonov",
    "email": "aparamonov@datagrok.ai"
  },
  "description": "ChaRPy translates viewers into Python or R scripts, executes them and displays the resulting plots.",
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/ChaRPy"
  },
  "dependencies": {
    "datagrok-api": "^1.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-google": "^0.14.0",
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "babel-loader": "^8.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "json5-loader": "^4.0.0"
  },
  "scripts": {
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "link-api": "npm link datagrok-api",
    "upload-debug-local": "webpack && grok publish http://localhost:8082 ",
    "upload-debug": "webpack && grok publish https://dev.datagrok.ai/api ",
    "upload-deploy": "webpack && grok publish https://dev.datagrok.ai/api --release",
    "build": "webpack"
  },
  "canEdit": [
    "Developers"
  ],
  "canView": [
    "All users"
  ],
  "category": "Visualizations"
}
