  "description": "Integration with the PubChem web service.",
  "version": "1.2.0",
  "author": {
    "name": "Maria Dolotova",
    "email": "mdolotova@datagrok.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/PubChemApi"
  },
  "dependencies": {
    "datagrok-api": "^1.21.1",
    "@datagrok-libraries/utils": "^4.3.0"
  },
  "devDependencies": {
    "ts-loader": "latest",
