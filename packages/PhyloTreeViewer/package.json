{
	"name": "@datagrok/phylotree-viewer",
	"friendlyName": "Phylo Tree",
	"version": "0.0.6",
	"description": "*PhyloTreeViewer* is a [Datagrok package](https://datagrok.ai/help/develop/develop#packages) for phylogenetic trees visualization.",
	"repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/PhyloTreeViewer"
  },
	"dependencies": {
		"datagrok-api": "^1.0.0"
	},
	"sources": [
		"phylotree/phylotree.css",
		"phylotree/jquery.js",
		"phylotree/bootstrap.min.js",
		"phylotree/d3.v3.min.js",
		"phylotree/phylotree.js",
		"phylotree/underscore-min.js"
	],
	"scripts": {
		"debug-phylotreeviewer": "grok publish --rebuild",
		"release-phylotreeviewer": "grok publish --rebuild --release",
		"build-phylotreeviewer": "webpack",
		"build": "webpack",
		"debug-phylotreeviewer-public": "grok publish public --rebuild",
		"release-phylotreeviewer-public": "grok publish public --rebuild --release",
		"debug-phylotreeviewer-local": "grok publish local --rebuild",
		"release-phylotreeviewer-local": "grok publish local --rebuild --release"
	},
	"canEdit": [
		"Developers"
	],
	"canView": [
		"All users"
	],
	"devDependencies": {
		"webpack-cli": "^4.9.2"
	},
  "category": "Visualizations"
}
