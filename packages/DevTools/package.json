{
  "name": "@datagrok/dev-tools",
  "friendlyName": "DevTools",
  "version": "1.3.30",
  "author": {
    "name": "Diana Onufriienko",
    "email": "donufriienko@datagrok.ai"
  },
  "description": "Tools for the Datagrok developers (TestManager, DevPanel). [Learn more](https://github.com/datagrok-ai/public/blob/master/packages/DevTools/README.md)",
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/DevTools"
  },
  "dependencies": {
    "@datagrok-libraries/utils": "^2.3.1",
    "codemirror": "^5.65.0",
    "datagrok-api": "^1.6.0",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@types/codemirror": "^5.60.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cash-dom": "^8.1.0",
    "css-loader": "^5.2.4",
    "eslint": "^8.21.0",
    "eslint-config-google": "^0.14.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "latest",
    "webpack-cli": "latest"
  },
  "scripts": {
    "link-api": "npm link datagrok-api",
    "link-utils": "npm link @datagrok-libraries/utils",
    "debug-dev-tools": "grok publish",
    "release-dev-tools": "grok publish --release",
    "build-dev-tools": "webpack",
    "build": "webpack",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "debug-dev-tools-public": "grok publish public",
    "release-dev-tools-public": "grok publish public --release",
    "debug-dev-tools-local": "grok publish local",
    "release-dev-tools-local": "grok publish local --release",
    "release-local": "grok publish local --release",
    "debug-local": "grok publish local",
    "test": "grok test",
    "test-dev": "grok test --host dev",
    "test-local": "grok test --host localhost"
  },
  "sources": [
    "common/codemirror/codemirror.js",
    "common/codemirror/codemirror.css"
  ],
  "canEdit": [
    "Developers"
  ],
  "canView": [
    "Developers"
  ],
  "category": "Development"
}
