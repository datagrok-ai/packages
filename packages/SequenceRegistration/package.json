{
  "name": "@datagrok/sequence-registration",
  "friendlyName": "Sequence Registration",
  "version": "0.0.1",
  "author": {
    "name": "Alexey Choposky",
    "email": "achopovsky@datagrok.ai"
  },
  "description": "Sequence Registration is a [package](https://datagrok.ai/help/develop/develop#packages) for the [Datagrok](https://datagrok.ai) platform, used for bulk registration of [oligonucleotide](https://en.wikipedia.org/wiki/Oligonucleotide) sequences.",
  "dependencies": {
    "datagrok-api": "latest",
    "cash-dom": "latest",
    "dayjs": "latest",
    "@datagrok-libraries/utils": "latest"
  },
  "devDependencies": {
    "datagrok-tools": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "ts-loader": "latest",
    "typescript": "latest",
    "eslint": "latest",
    "eslint-config-google": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest"
  },
  "scripts": {
    "link-all": "",
    "debug-sequenceregistration": "webpack && grok publish",
    "release-sequenceregistration": "webpack && grok publish --release",
    "build-sequenceregistration": "webpack",
    "build": "webpack",
    "debug-sequenceregistration-dev": "webpack && grok publish dev",
    "release-sequenceregistration-dev": "webpack && grok publish dev --release",
    "debug-sequenceregistration-dev2": "webpack && grok publish dev2",
    "release-sequenceregistration-dev2": "webpack && grok publish dev2 --release",
    "debug-sequenceregistration-public": "webpack && grok publish public",
    "release-sequenceregistration-public": "webpack && grok publish public --release",
    "debug-sequenceregistration-local2": "webpack && grok publish local2",
    "release-sequenceregistration-local2": "webpack && grok publish local2 --release",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "test": "grok test"
  },
  "canEdit": [
    "Developers"
  ],
  "canView": [
    "All users"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com-work:datagrok-ai/public.git",
    "directory": "packages/SequenceRegistration"
  }
}
