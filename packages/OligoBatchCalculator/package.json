{
  "name": "@datagrok/oligo-batch-calculator",
  "beta": false,
  "friendlyName": "Oligo Batch Calculator",
  "version": "1.0.0",
  "description": "Calculates the following oligonucleotide properties: Length, Optical Density, NMoles, Mass, Molecular Weight, Extinction Coefficient (using nearest neighbour method)",
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/OligoBatchCalculator"
  },
  "dependencies": {
    "datagrok-api": "^1.5.3",
    "ts-loader": "latest",
    "typescript": "latest",
    "@datagrok-libraries/utils": "^0.2.9"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "cash-dom": "^8.1.0",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "@types/jest": "^27.0.0",
    "jest": "^27.0.0",
    "jest-html-reporter": "^3.5.0",
    "puppeteer": "^13.7.0",
    "ts-jest": "^27.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node-fetch": "^2.6.1"
  },
  "scripts": {
    "debug-oligobatchcalculator": "grok publish",
    "release-oligobatchcalculator": "grok publish --release",
    "build-oligobatchcalculator": "webpack",
    "build": "webpack",
    "debug-oligobatchcalculator-public": "grok publish public",
    "release-oligobatchcalculator-public": "grok publish public --release",
    "debug-oligobatchcalculator-local": "grok publish local",
    "release-oligobatchcalculator-local": "grok publish local --release",
    "test": "set HOST=dev && jest",
    "test-dev": "set HOST=dev && jest",
    "test-local": "set HOST=localhost && jest"
  },
  "sources": [
    "css/style.css"
  ],
  "canEdit": [
    "Developers"
  ],
  "canView": [
    "All users"
  ],
  "category": "Bioinformatics"
}
