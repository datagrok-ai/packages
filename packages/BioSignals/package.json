{
  "name": "@datagrok/bio-signals",
  "fullName": "BioSignals",
  "version": "0.0.5",
  "description": "BioSignals is a package for the [Datagrok](https://datagrok.ai) platfrom. The goal of the project is to offer an efficient and automated biosignal processing routine.",
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/BioSignals"
  },
  "dependencies": {
    "datagrok-api": "^1.0.0",
    "echarts": "^5.0.2"
  },
  "scripts": {
    "link-api": "npm link datagrok-api",
    "debug-biosignals": "grok publish --rebuild",
