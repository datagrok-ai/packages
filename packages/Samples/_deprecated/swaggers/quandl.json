        "parameters": [
          {
            "name": "Cumulative",
            "description": "y’[t] = y[t] +y[t-1] + … + y[0]",
            "required": true,
            "in": "query",
            "type": "string"
          },
          {
            "name": "Row-on-row",
            "description": "y’[t] = y[t] - y[t-1]",
            "required": true,
            "in": "query",
            "type": "string"
          },
          {
            "name": "Start",
            "description": "y’[t] = (y[t]/y[0]) * 100",
