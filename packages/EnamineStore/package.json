{
  "name": "@datagrok/enamine-store",
  "version": "1.0.0",
  "author": {
    "name": "Volodymyr Dyma",
    "email": "vdyma@datagrok.ai"
  },
  "fullName": "Enamine Store",
  "description": "Integration with Enamine. Allows to search for molecules available in Enamine, and provides contextual information for the current molecule.",
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/EnamineStore"
  },
  "dependencies": {
    "datagrok-api": "latest",
    "@datagrok-libraries/utils": "latest"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "css-loader": "latest",
    "eslint": "latest",
    "eslint-config-google": "latest",
    "style-loader": "latest",
    "ts-loader": "latest",
    "typescript": "latest",
    "datagrok-tools": "latest",
    "webpack": "latest",
    "webpack-cli": "latest"
  },
  "scripts": {
    "link-api": "npm link datagrok-api",
    "link-utils": "npm link @datagrok-libraries/utils",
    "link-all": "npm link datagrok-api @datagrok-libraries/utils",
    "install-dependencies": "npm install",
    "debug-peptides": "grok publish",
    "release-peptides": "grok publish --release",
    "build-peptides": "webpack",
    "build": "webpack",
    "debug-peptides-dev": "grok publish dev",
    "release-peptides-dev": "grok publish dev --release",
    "debug-peptides-public": "grok publish public",
    "release-peptides-public": "grok publish public --release",
    "debug-peptides-local": "grok publish local",
    "release-peptides-local": "grok publish local --release",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint-fix": "eslint \"./src/**/*.ts\" --fix",
    "test": "grok test",
    "test-dev": "grok test --host dev",
    "test-local": "grok test --host localhost"
  },
  "sources": [
    "css/enamine_store.css"
  ],
  "canEdit": [ "Developers" ],
  "canView": [ "All users" ],
  "category": "Cheminformatics"
}
