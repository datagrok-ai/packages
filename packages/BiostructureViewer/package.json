{
  "name": "@datagrok/biostructure-viewer",
  "friendlyName": "BiostructureViewer",
  "author": {
    "name": "Aleksandr Tanas",
    "email": "atanas@datagrok.ai"
  },
  "version": "0.2.12",
  "description": "Protein viewers: support for multiple file formats, stand-alone viewer, and in-grid viewer. [Learn more](https://github.com/datagrok-ai/public/blob/master/packages/BiostructureViewer/README.md)",
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/BiostructureViewer"
  },
  "sources": [
    "css/ngl.css",
    "css/pviz-core.css",
    "css/additionalStyles.css",
    "css/pviz-bundle.min.js",
    "common/ngl_viewer/ngl.js",
    "css/molstar.css"
  ],
  "dependencies": {
    "@datagrok-libraries/bio": "^5.26.0",
    "@datagrok-libraries/utils": "^2.5.0",
    "@rcsb/rcsb-molstar": "^1.8.7",
    "@rcsb/rcsb-api-tools": "^4.1.1",
    "cash-dom": "^8.0.0",
    "datagrok-api": "^1.10.2",
    "dayjs": "^1.11.4",
    "molstar": "^2.4.1",
    "rxjs": "^6.5.5",
    "wu": "latest"
  },
  "devDependencies": {
    "@types/node": "^17.0.24",
    "@types/wu": "latest",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.5.2",
    "sass": "^1.49.0",
    "sass-loader": "^12.4.0",
    "ts-loader": "^9.2.5",
    "tslib": "^2.5.0",
    "typescript": "^4.2.3",
    "webpack": "^5.76.0",
    "webpack-bundle-analyzer": "latest",
    "webpack-cli": "^4.6.0"
  },
  "grokDependencies": {
    "@datagrok/chem": "1.5.3"
  },
  "scripts": {
    "link-api": "npm link datagrok-api",
    "link-utils": "npm link @datagrok-libraries/utils",
    "link-all": "npm link datagrok-api @datagrok-libraries/utils @datagrok-libraries/bio",
    "debug-biostructureviewer": "grok publish",
    "release-biostructureviewer": "grok publish --release",
    "build-biostructureviewer": "webpack",
    "build": "webpack",
    "debug-biostructureviewer-dev": "grok publish dev",
    "release-biostructureviewer-dev": "grok publish dev --release",
    "debug-biostructureviewer-public": "grok publish public",
    "release-biostructureviewer-public": "grok publish public --release",
    "debug-biostructureviewer-local": "grok publish local",
    "release-biostructureviewer-local": "grok publish local --release",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "test": "grok test",
    "test-dev": "grok test --host dev",
    "test-localhost": "grok test --host localhost",
    "analyze": "webpack --profile --json > ./stats.json && npx webpack-bundle-analyzer ./stats.json"
  },
  "canEdit": [
    "Developers"
  ],
  "canView": [
    "All users"
  ],
  "category": "Bioinformatics"
}
